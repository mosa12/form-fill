{
  "manifest_version": 3,
  "name": "Smart Form Filler",
  "version": "1.0",
  "description": "Auto-fill forms using trained ML models.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/icons.png"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}